{
  "name": "brady-ai",
  "version": "1.1.0",
  "description": "Multi-provider AI gateway with intelligent routing and cost optimization",
  "type": "module",
  "main": "dist/openai-api-server.js",
  "bin": {
    "brady-cli": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "brady": "npm run build && node dist/openai-api-server.js",
    "brady:server": "npm run build && node dist/http-server.js",
    "dev": "tsx src/openai-api-server.ts",
    "test": "tsx src/test.ts",
    "cli": "tsx src/cli.ts",
    "install-cli": "npm run build && npm link"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.27.0",
    "@google/generative-ai": "^0.21.0",
    "@modelcontextprotocol/sdk": "^0.5.0",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "groq-sdk": "^0.27.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.67.1",
    "portkey-ai": "^1.10.1",
    "ws": "^8.18.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^22.0.0",
    "@types/ws": "^8.5.12",
    "tsx": "^4.7.0",
    "typescript": "^5.6.0"
  },
  "keywords": [
    "ai",
    "llm",
    "gateway",
    "multi-provider",
    "portkey",
    "openai",
    "anthropic",
    "groq"
  ],
  "author": "Brady AI Team",
  "license": "MIT"
}
